{
  "name": "climatum-react-seed",
  "version": "0.0.1",
  "description": "A React Seed app",
  "author": "Sam Bleckley <sam@climatum.com>",
  "homepage": "https://gitlab.com/climatum/kepler.git",
  "license": "UNLICENSED",
  "repository": "https://",
  "scripts": {
    "build-dev": "webpack --display-error-details --config build_scripts/webpack.config.dev.js",
    "build": "webpack --display-error-details --config build_scripts/webpack.config.prod.js",
    "watch": "webpack --watch --config build_scripts/webpack.config.dev.js",
    "generate": "plop --plopfile build_scripts/plopfile.js",
    "start": "webpack-dev-server --config build_scripts/webpack.config.dev.js --progress --colors",
    "test": "jest",
    "deploy": "./build_scripts/deploy.sh"
  },
  "dependencies": {
    "@types/react-dom": "^15.5.4",
    "@types/react-transition-group": "^2.0.4",
    "actioncable": "^5.1.1",
    "chroma-js": "^1.3.4",
    "dashify": "^0.2.2",
    "deep-equal": "^1.0.1",
    "draft-js": "^0.10.1",
    "draft-js-ast-exporter": "^1.0.0",
    "draft-js-ast-importer": "^1.0.0",
    "es6-promise-polyfill": "^1.2.0",
    "font-awesome": "^4.7.0",
    "glamor": "^2.20.24",
    "he": "^1.1.1",
    "history": "^4.6.1",
    "immutable": "^3.8.1",
    "lodash.debounce": "^4.0.8",
    "mobx": "^3.1.10",
    "mobx-react": "^4.2.1",
    "moment": "^2.18.1",
    "prop-types": "^15.5.8",
    "react": "^15.6.1",
    "react-dates": "^12.2.2",
    "react-document-title": "^2.0.3",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "@types/actioncable": "^0.0.2",
    "@types/chroma-js": "^1.3.0",
    "@types/draft-js": "^0.10.10",
    "@types/enzyme": "^2.7.9",
    "@types/he": "^0.5.29",
    "@types/history": "^4.5.0",
    "@types/jest": "^19.2.2",
    "@types/lodash": "^4.14.41",
    "@types/lodash.debounce": "^4.0.2",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^16.0.10",
    "@types/react-addons-test-utils": "^0.14.15",
    "@types/react-hot-loader": "^3.0.0",
    "@types/requirejs": "^2.1.29",
    "awesome-typescript-loader": "^3.0.0-beta.9",
    "css-loader": "^0.25.0",
    "css-to-string-loader": "^0.1.2",
    "enzyme": "^2.8.2",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "inquirer-directory": "^2.1.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "plop": "^1.7.2",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.5.4",
    "source-map-support": "^0.4.6",
    "style-loader": "^0.13.1",
    "ts-jest": "^20.0.5",
    "typescript": "^2.5.3",
    "uglify-js": "^3.0.20",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^2.5.0",
    "webpack-hot-middleware": "^2.13.2"
  },
  "engines": {
    "node": ">= 7.6.0",
    "npm": ">= 3"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/app"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg|css)$": "<rootDir>/build_scripts/jest/fileMock.js",
      "\\./history$": "<rootDir>/app/router/__mocks__/history.ts",
      "\\./(.*)-service$": "<rootDir>/app/models/$1/__mocks__/$1-service.ts"
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/build_scripts/jest/preprocessor.js"
    },
    "testRegex": "app/.*\\.spec\\.(ts|tsx)$",
    "globals": {
      "PRODUCTION": false
    }
  }
}
